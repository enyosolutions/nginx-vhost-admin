{
  "private": true,
  "name": "enyo-server-agent",
  "description": "a Sails application",
  "license": "",
  "author": "faou",
  "version": "0.1.0",
  "main": "app.js",
  "scripts": {
    "postinstall": "node scripts/postInstall.js",
    "pre-commit": "husky",
    "precommit": "husky",
    "debug": "node --inspect app.js",
    "dev": "nodemon --inspect app.js",
    "lint": "./node_modules/eslint/bin/eslint.js api config",
    "lint:fix": "./node_modules/eslint/bin/eslint.js api config --fix",
    "lint:fix:q": "./node_modules/eslint/bin/eslint.js api config --fix --quiet",
    "start": "node app.js",
    "serve": "node app.js",
    "test": "node ./node_modules/mocha/bin/mocha test/bootstrap.test.js test/integration/**/*.test.js"
  },
  "dependencies": {
    "@webantic/nginx-config-parser": "^1.3.0",
    "ajv": "^5.3.0",
    "async": "^2.0.0-rc.6",
    "atob": "^2.0.0",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "btoa": "^1.1.2",
    "colors": "^1.3.2",
    "compression": "^1.7.4",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "ejs": "~0.8.4",
    "eslint": "^4.19.1",
    "express": "^4.16.3",
    "express-basic-auth": "^1.2.0",
    "hash-sum": "^1.0.2",
    "js-base64": "^2.4.9",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^7.1.6",
    "lodash": "^4.17.5",
    "mkdirp": "^0.5.1",
    "moment": "^2.11.0",
    "node-cache": "^4.2.1",
    "object-assign": "^4.1.1",
    "q": "^1.4.1",
    "rc": "~0.5.0",
    "sails": "^1.0.2",
    "sails-generate-views": "^0.10.8",
    "sails-hook-sockets": "^2.0.0",
    "shelljs": "^0.8.2",
    "tracer": "^0.9.1"
  },
  "devDependencies": {
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "faker": "^4.1.0",
    "husky": "^5.1.3",
    "lint-staged": "^7.2.2",
    "mocha": "^4.0.1",
    "mocha-clean": "^1.0.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.1.3",
    "prettier-package-json": "^2.0.0",
    "supertest": "^3.0.0"
  },
  "keywords": [],
  "engines": {
    "node": ">=9.11.2"
  },
  "lint-staged": {
    "linters": {
      "package.json": [
        "git add .",
        "prettier-package-json --write"
      ],
      "front/package.json": [
        "git add .",
        "prettier-package-json --write"
      ],
      "api/**/*.js": [
        "yarn lint:fix",
        "git add"
      ]
    }
  },
  "nodemonConfig": {
    "ignore": [
      "front/*",
      "docs/*",
      "test/*"
    ],
    "delay": "200"
  }
}
